@model IEnumerable<NavyAccountCore.Models.InvestmentView>
@{
    ViewData["Title"] = "outstanding";
}

<div class="header">
    <h1 class="header-title">
        OutStanding Investment
    </h1>
    <nav aria-label="breadcrumb">
        <ol class="breadcrumb">
            <li class="breadcrumb-item"><a href="Home/Index">Main</a></li>
            <li class="breadcrumb-item active" aria-current="page">OutStanding Investment</li>
        </ol>
    </nav>
</div>
<div class="row">
    <div class="col-12">
        <div class="card">
            <div class="card-header">
                <h5 class="card-title">OutStanding Investment</h5>
            </div>

            @TempData["message"]
            <div class="card-body">


                <table class="table table-striped table-bordered">
                    <tr>
                        <th>Investment Type</th>
                        <th>MatureDate</th>
                        <th>Issuance Bank</th>
                        <th>Receiving Bank</th>
                        <th>Amount</th>
                        <th>Interest</th>
                        <th>Tenure</th>

                    </tr>
                    @if (Model != null)
                    {
                        @foreach (var a in Model)
                        {
                            <tr>
                                <td>@a.InvestmentType</td>
                                <td>@a.Maturingdate</td>
                                <td>@a.issuancebank </td>
                                <td>@a.receivingbank</td>
                                <td>@a.Amount</td>
                                <td>@a.interest</td>
                                <td>@a.Tenure</td>

                            </tr>
                        }
                    }
                </table>



            </div>



        </div>
    </div>
</div>
