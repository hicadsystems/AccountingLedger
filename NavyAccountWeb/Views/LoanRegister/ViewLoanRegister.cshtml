@using Microsoft.AspNetCore.Http;
@{
    ViewBag.Title = "ViewFinancial";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<div class="header">
    <h1 class="header-title">
        Loan Register
    </h1>
    <nav aria-label="breadcrumb">
        <ol class="breadcrumb">
            <li class="breadcrumb-item"><a href="Home/Index">Main</a></li>
            <li class="breadcrumb-item active" aria-current="page">Loan List</li>
        </ol>
    </nav>
</div>


<update-loan-register-component></update-loan-register-component>

<div class="row">
    <div class="col-12">
        <div class="card">
            <div class="card-header">
                <h5 class="card-title">Loan Register Progressing</h5>
            </div>

            <view-loan-register-component fundcodeid="@Context.Session.GetInt32("fundtypeid")" statusdef="1" isapplication="progress"></view-loan-register-component>
        </div>
    </div>
</div>


