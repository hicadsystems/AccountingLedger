@model IEnumerable<NavyAccountCore.Models.MainAccountView>
@using Microsoft.AspNetCore.Http;
@{
    Layout = null;

}
<!DOCTYPE html>
<html>
<body>
    <h2 style="text-align:center">NIGERIAN NAVY DIRECTORATE OF NON-PUBLIC FUND</h2>
    <h2 style="text-align:center">NAVIP, DEPENDANT FUND AND WELFARE LOAN PORTFOLIOS</h2>
    Main Account: Produce On @DateTime.Now
    <table border="2" cellpadding="2" cellspacing="0" width="100%">

        <tr>
            <th>Code</th>
            <th>Name</th>
            <th>Balance sheet</th>
            <th>SubType</th>

        </tr>
        @foreach (var a in Model)
        {
            <tr>
                <td> @a.maincode </td>
                <td> @a.description </td>
                <td> @a.balSheetdesc</td>
                <td> @a.subtypeDesc </td>
            </tr>
        }
    </table>
</body>
</html>