@model IEnumerable<NavyAccountWeb.ViewModels.NPFContributionsViewModel>

    @{
    ViewData["Title"] = "Index";
    }

    <div class="header">
        <h1 class="header-title">
            Pay Per-Rank
        </h1>
        <nav aria-label="breadcrumb">
            <ol class="breadcrumb">
                <li class="breadcrumb-item"><a href="/Home/Index">Main</a></li>
                <li class="breadcrumb-item active" aria-current="page">Payperrank</li>
            </ol>
        </nav>
    </div>
    <div class="row">
        <div class="col-12">
            <div class="card">
                <div class="card-header">
                    <h5 class="card-title">Create Payperrank</h5>
                </div>
                @using (Html.BeginForm("CreateFinancial", "BalanceSheet", FormMethod.Post))
                {

                @Html.AntiForgeryToken()
                

                 <create-contribution-component></create-contribution-component>
                
                }

            </div>
        </div>

    </div>

    <div class="row">
        <div class="col-12">
            <div class="card">
                <div class="card-header">
                    <h5 class="card-title">Payperrank List</h5>
                </div>

                <view-contribution-component></view-contribution-component>
            </div>
        </div>
    </div>



